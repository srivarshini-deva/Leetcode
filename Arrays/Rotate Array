class Solution {
public:

    void reversal(vector<int>& arr, int a, int b){
        while(a < b){
            int temp = arr[a];
            arr[a] = arr[b];
            arr[b] = temp;
            a++;
            b--;
        }
    }
    void rotate(vector<int>& nums, int k) {
        int n = nums.size();
        k %= n;
        reversal(nums, 0, n-1);
        reversal(nums, 0, k-1);
        reversal(nums, k, n-1);
    }
};
